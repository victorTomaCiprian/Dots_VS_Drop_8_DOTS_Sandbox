.blackboardFieldPropertyView > .rowView {
    flex-direction: row;
    padding-left: 10px;
    padding-right: 2px;
    padding-top: 2px;
    padding-bottom: 2px;
}

.blackboardFieldPropertyView > .rowView > .rowViewLabel {
    min-width: 50px;
    font-size: 11px;
}

.blackboardFieldPropertyView * {
    font-size: 11px;
}

.blackboardFieldPropertyView > .rowView > .rowViewControl {
    flex: 1;
}

.stack-node > #stackNodeHeaderContainer > * > #stackNodeTitleContainer > #titleField {
    position: absolute;
}

.stack-node > #stackNodeHeaderContainer > * > #stackNodeTitleContainer > #titleLabel {
    overflow: hidden;
    text-overflow: ellipsis;
    -unity-text-overflow-position: end;
    white-space: nowrap;
}

.stack-node #stackNodeHeaderContainer {
    border-radius: 8px;
    background-color: #252525;
    padding-left: 8px;
    padding-right: 8px;
    padding-top: 8px;
    padding-bottom: 8px;
}

.stack-node #stackNodeHeaderContainer EnumField {
    height: 20px;
    font-size: 12px;
}
